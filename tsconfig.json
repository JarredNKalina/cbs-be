{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"outDir": "./dist",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"strict": true,
		"strictNullChecks": true,
		"resolveJsonModule": true
	},
	"include": ["src", "firebase.config.js"],
	"exclude": ["node_modules"]
}
